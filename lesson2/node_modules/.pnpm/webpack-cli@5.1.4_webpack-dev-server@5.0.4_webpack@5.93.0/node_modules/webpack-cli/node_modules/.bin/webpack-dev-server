#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/parvin/Desktop/CA/lesson2/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack-cli@5.1.4_webpack@5.93.0/node_modules/webpack-dev-server/bin/node_modules:/Users/parvin/Desktop/CA/lesson2/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack-cli@5.1.4_webpack@5.93.0/node_modules/webpack-dev-server/node_modules:/Users/parvin/Desktop/CA/lesson2/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack-cli@5.1.4_webpack@5.93.0/node_modules:/Users/parvin/Desktop/CA/lesson2/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/parvin/Desktop/CA/lesson2/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack-cli@5.1.4_webpack@5.93.0/node_modules/webpack-dev-server/bin/node_modules:/Users/parvin/Desktop/CA/lesson2/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack-cli@5.1.4_webpack@5.93.0/node_modules/webpack-dev-server/node_modules:/Users/parvin/Desktop/CA/lesson2/node_modules/.pnpm/webpack-dev-server@5.0.4_webpack-cli@5.1.4_webpack@5.93.0/node_modules:/Users/parvin/Desktop/CA/lesson2/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../webpack-dev-server@5.0.4_webpack-cli@5.1.4_webpack@5.93.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
else
  exec node  "$basedir/../../../../../webpack-dev-server@5.0.4_webpack-cli@5.1.4_webpack@5.93.0/node_modules/webpack-dev-server/bin/webpack-dev-server.js" "$@"
fi
